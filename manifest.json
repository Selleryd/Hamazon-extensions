{
  "manifest_version": 3,
  "name": "Hamazon Product Verifier",
  "version": "1.0",
  "description": "Verify products against your Hamazon certification database",
  "permissions": [],
  "host_permissions": [
    "https://script.google.com/macros/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}
